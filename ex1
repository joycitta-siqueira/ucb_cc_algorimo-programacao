#include<stdio.h>
int main(){ 
	int numero1, numero2, soma;
	char sexo, nome[4];
	float nota;
	double media;
	bool presenca;
	
	printf("Digite o numero 1: ");
	scanf("%d", &numero1);
	printf("Digite o sexo: ");
	scanf("%c", &sexo);
	printf("Digite o nome: ");
	scanf("%s", &nome);
	printf("Digite a nota: ");
	scanf("%f", &nota);
	printf("Digite a media: ");
	scanf("%lf", &media);
	printf("Digite a presenca: ");
	scanf("%b", &presenca);	
	
}
